<div class="container">
  <div class="row justify-content-md-center pt-5">
    <div class="col-md-auto">
      <div><%= form_with(url: search_vaccination_logs_path, local: true, method: :get, class: "search-form") do |form| %>
           <%= form.text_field :keyword, placeholder: "検索", class: "search-input" %>
           <%= form.submit "検索", class: "search-btn" %>
           <% end %>
      </div>
    </div>
  </div>
</div>
<div class="contents row">
  <% @vaccination_logs.each do |vaccination_log| %>
    <%= render partial: "vaccination_log", locals: { vaccination_log: vaccination_log } %>
  <% end %>
</div>